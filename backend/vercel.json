{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
        "src": "backend/api/index.js",
        "use": "@vercel/node"
        },
        {
        "src": "src/**/**",
        "use": "@vercel/static"
        }
    ],
     "routes": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}