{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
        "src": "backend/api/index.js",
        "use": "@vercel/node"
        },
        {
        "src": "frontend/dist/**",
        "use": "@vercel/static-build"
        }
    ],
     "routes": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}